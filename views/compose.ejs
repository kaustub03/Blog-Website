<%- include("./partials/header")  %> 
<h1>Compose</h1>
<form action="/compose" class="item" method="post">
    <div class="form-group">
        <h4>Title</h4>
        <input type="text" name="postTitle" class="form-control">
        <h4>Post</h4>
        <textarea name="postBody" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>  
<%- include("./partials/footer")  %> 